   "source": [
    "\"Age Demographics Segment\"\n",
    "\n",
    "\"Establish Bins for dataframe\"\n",
    "bins = [0,10,15,20,25,30,35,40,999]\n",
    "bin_names = ['<10', '10-14', '15-19', '20-24', '25-29', '30-34','35-39', '40+']\n",
    "\n",
    "\"Get unique player names\"\n",
    "unique_players = demographic.loc[:,[\"SN\", \"Age\"]]\n",
    "unique_players[\"Age Range\"] = pd.cut(unique_players[\"Age\"], bins,labels=bin_names)\n",
    "\n",
    "\"Calculations for bins\"\n",
    "age_demographic_total = unique_players[\"Age Range\"].value_counts()\n",
    "age_demographic_percentage = (age_demographic_total/player_count*100).round(2)\n",
    "\n",
    "\"Summary table for bins\"\n",
    "\n",
    "summary_for_bins = pd.DataFrame({\"Percentage of Players\": age_demographic_percentage,\n",
    "                                \"Total Count\": age_demographic_total})\n",
    "summary_for_bins.sort_index()"
   ]
  },
